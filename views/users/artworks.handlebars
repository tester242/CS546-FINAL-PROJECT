<article>
    <header>
        <h1>About Maggie</h1>
        <p>*Insert a little bio by Maggie*</p>
    </header>
</article>

<div class="gallery">
{{#each artworks}}
    <div class="icon">
        <div class="iconInner">
            <a href="/artwork"><img src={{this.artImage}} alt="{{this.name}}"></a>
            <div class="artworkTitle">{{this.name}}</div>
        </div>
    </div>
{{/each}}
</div>

<footer>
    <a href="/artworks">Back to Top</a>
    {{#if loggedIn}}
        <a href="/logout">Logout</a>
    {{/if}}
</footer>